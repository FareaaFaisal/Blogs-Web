[32m+[m[32m        className={`w-6 h-6 rotate-0 scale-100 transition-all  dark:rotate-90  dark:hidden `}[m
[32m+[m[32m      />[m
[32m+[m[32m    </button>[m
[32m+[m[32m  );[m
[32m+[m[32m}[m
