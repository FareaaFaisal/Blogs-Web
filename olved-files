[1mdiff --git a/src/app/blog/[slug]/commentSec/page.tsx b/src/app/blog/[slug]/commentSec/page.tsx[m
[1mnew file mode 100644[m
[1mindex 0000000..cd32ef0[m
[1m--- /dev/null[m
[1m+++ b/src/app/blog/[slug]/commentSec/page.tsx[m
[36m@@ -0,0 +1,70 @@[m
[32m+[m[32m"use client"[m
[32m+[m[32mimport React, { useState, useEffect } from "react";[m
[32m+[m
[32m+[m[32mconst CommentSec = () => {[m
[32m+[m[32m  const [comments, setComments] = useState<{ username: string; comment: string }[]>([]);[m
[32m+[m[32m  const [username, setUsername] = useState("");[m
[32m+[m[32m  const [comment, setComment] = useState("");[m
[32m+[m
[32m+[m[32m  // Load comments from local storage on mount[m
[32m+[m[32m  useEffect(() => {[m
[32m+[m[32m    const savedComments = JSON.parse(localStorage.getItem("comments") || "[]");[m
[32m+[m[32m    setComments(savedComments);[m
[32m+[m[32m  }, []);[m
[32m+[m
[32m+[m[32m  // Save comments to local storage whenever comments change[m
[32m+[m[32m  useEffect(() => {[m
[32m+[m[32m    localStorage.setItem("comments", JSON.stringify(comments));[m
[32m+[m[32m  }, [comments]);[m
[32m+[m
[32m+[m[32m  const handleAddComment = () => {[m
[32m+[m[32m    if (username.trim() && comment.trim()) {[m
[32m+[m[32m      const newComment = { username, comment };[m
[32m+[m[32m      setComments([...comments, newComment]);[m
[32m+[m[32m      setUsername("");[m
[32m+[m[32m      setComment("");[m
[32m+[m[32m    }[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  return ([m
[32m+[m[32m    <div className="max-w-2xl mx-auto p-4">[m
[32m+[m[32m      <h1 className="text-2xl font-bold mb-4">Comment Section</h1>[m
[32m+[m[32m      <div className="mb-4">[m
[32m+[m[32m        <input[m
[32m+[m[32m          type="text"[m
[32m+[m[32m          placeholder="Your name"[m
[32m+[m[32m          value={username}[m
[32m+[m[32m          onChange={(e) => setUsername(e.target.value)}[m
[32m+[m[32m          className="w-full p-2 border border-gray-300 rounded mb-2 focus:outline-none focus:ring-2 focus:ring-blue-500"[m
[32m+[m[32m        />[m
[32m+[m[32m        <textarea[m
[32m+[m[32m          placeholder="Your comment"[m
[32m+[m[32m          value={comment}[m
[32m+[m[32m          onChange={(e) => setComment(e.target.value)}[m
[32m+[m[32m          className="w-full p-2 border border-gray-300 rounded mb-2 focus:outline-none focus:ring-2 focus:ring-blue-500"[m
[32m+[m[32m          rows={4}[m
[32m+[m[32m        ></textarea>[m
[32m+[m[32m        <button[m
[32m+[m[32m          onClick={handleAddComment}[m
[32m+[m[32m          className="px-4 py-2 bg-blue-500 text-white rounded hover:bg-blue-600"[m
[32m+[m[32m        >[m
[32m+[m[32m          Add Comment[m
[32m+[m[32m        </button>[m
[32m+[m[32m      </div>[m
[32m+[m
[32m+[m[32m      <div className="space-y-4">[m
[32m+[m[32m        {comments.map((c, index) => ([m
[32m+[m[32m          <div[m
[32m+[m[32m            key={index}[m
[32m+[m[32m            className="p-4 border border-gray-300 rounded shadow-sm"[m
[32m+[m[32m          >[m
[32m+[m[32m            <h2 className="font-bold text-lg">{c.username}</h2>[m
[32m+[m[32m            <p className="text-gray-700">{c.comment}</p>[m
[32m+[m[32m          </div>[m
[32m+[m[32m        ))}[m
[32m+[m[32m      </div>[m
[32m+[m[32m    </div>[m
[32m+[m[32m  );[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mexport default CommentSec;[m
