[32m+[m[32m/** @type {import('next').NextConfig} */[m
[32m+[m[32mconst nextConfig = {[m
[32m+[m[32m    images:{[m
[32m+[m[32m        remotePatterns:[[m
[32m+[m[32m            {[m
[32m+[m[32m                protocol:'https',[m
[32m+[m[32m                hostname:'cdn.sanity.io'[m
[32m+[m[32m            }[m
[32m+[m[32m        ][m
[32m+[m[32m    }[m
[32m+[m[32m};[m
