[32m+[m[32m<!DOCTYPE html><html lang="en">[m
[32m+[m[32m<!--[m
[32m+[m[32mThis file is auto-generated from "sanity dev".[m
[32m+[m[32mModifications to this file are automatically discarded.[m
[32m+[m[32m-->[m
[32m+[m[32m<head><meta charSet="utf-8"/><meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, viewport-fit=cover"/><meta name="robots" content="noindex"/><meta name="referrer" content="same-origin"/><link rel="icon" href="/static/favicon.ico" sizes="any"/><link rel="icon" href="/static/favicon.svg" type="image/svg+xml"/><link rel="apple-touch-icon" href="/static/apple-touch-icon.png"/><link rel="manifest" href="/static/manifest.webmanifest"/><title>Sanity Studio</title><script>[m
[32m+[m[32m;(function () {[m
[32m+[m[32m  var _caughtErrors = [][m
[32m+[m
[32m+[m[32m  var errorChannel = (function () {[m
[32m+[m[32m    var subscribers = [][m
[32m+[m
[32m+[m[32m    function publish(msg) {[m
[32m+[m[32m      for (var i = 0; i < subscribers.length; i += 1) {[m
[32m+[m[32m        subscribers[i](msg)[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    function subscribe(subscriber) {[m
[32m+[m[32m      subscribers.push(subscriber)[m
[32m+[m
[32m+[m[32m      return function () {[m
[32m+[m[32m        var idx = subscribers.indexOf(subscriber)[m
[32m+[m
[32m+[m[32m        if (idx > -1) {[m
[32m+[m[32m          subscribers.splice(idx, 1)[m
[32m+[m[32m        }[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return {publish, subscribe, subscribers}[m
[32m+[m[32m  })()[m
[32m+[m
[32m+[m[32m  // NOTE: Store the error channel instance in the global scope so that the Studio application can[m
[32m+[m[32m  // access it and subscribe to errors.[m
[32m+[m[32m  window.__sanityErrorChannel = {[m
[32m+[m[32m    subscribe: errorChannel.subscribe,[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  function _nextTick(callback) {[m
[32m+[m[32m    setTimeout(callback, 0)[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  function _handleError(error, params) {[m
[32m+[m[32m    _nextTick(function () {[m
[32m+[m[32m      // - If there are error channel subscribers, then we notify them (no console error).[m
[32m+[m[32m      // - If there are no subscribers, then we log the error to the console and render the error overlay.[m
[32m+[m[32m      if (errorChannel.subscribers.length) {[m
[32m+[m[32m        errorChannel.publish({error, params})[m
[32m+[m[32m      } else {[m
[32m+[m[32m        console.error(error)[m
[32m+[m
[32m+[m[32m        _renderErrorOverlay(error, params)[m
[32m+[m[32m      }[m
[32m+[m[32m    })[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  var ERROR_BOX_STYLE = [[m
[32m+[m[32m    'background: #fff',[m
[32m+[m[32m    'border-radius: 6px',[m
[32m+[m[32m    'box-sizing: border-box',[m
[32m+[m[32m    'color: #121923',[m
[32m+[m[32m    'flex: 1',[m
[32m+[m[32m    "font-family: -apple-system,BlinkMacSystemFont,'Segoe UI',Roboto,'Helvetica Neue','Liberation Sans',Helvetica,Arial,system-ui,sans-serif",[m
[32m+[m[32m    'font-size: 16px',[m
[32m+[m[32m    'line-height: 21px',[m
[32m+[m[32m    'margin: 0 auto',[m
[32m+[m[32m    'max-width: 960px',[m
[32m+[m[32m    'overflow: auto',[m
[32m+[m[32m    'padding: 20px',[m
[32m+[m[32m    'width: 100%',[m
[32m+[m[32m  ].join(';')[m
[32m+[m
[32m+[m[32m  var ERROR_CODE_STYLE = [[m
[32m+[m[32m    'color: #972E2A',[m
[32m+[m[32m    "font-family: -apple-system-ui-monospace, 'SF Mono', Menlo, Monaco, Consolas, monospace",[m
[32m+[m[32m    'font-size: 13px',[m
[32m+[m[32m    'line-height: 17px',[m
[32m+[m[32m    'margin: 0',[m
[32m+[m[32m  ].join(';')[m
[32m+[m
[32m+[m[32m  function _renderErrorOverlay(error, params) {[m
[32m+[m[32m    var errorElement = document.querySelector('#__sanityError') || document.createElement('div')[m
[32m+[m[32m    var colno = params.event.colno[m
[32m+[m[32m    var lineno = params.event.lineno[m
[32m+[m[32m    var filename = params.event.filename[m
[32m+[m
[32m+[m[32m    errorElement.id = '__sanityError'[m
[32m+[m[32m    errorElement.innerHTML = [[m
[32m+[m[32m      '<div style="' + ERROR_BOX_STYLE + '">',[m
[32m+[m[32m      '<div style="font-weight: 700;">Uncaught error: ' + error.message + '</div>',[m
[32m+[m[32m      '<div style="color: #515E72; font-size: 13px; line-height: 17px; margin: 10px 0;">' +[m
[32m+[m[32m        filename +[m
[32m+[m[32m        ':' +[m
[32m+[m[32m        lineno +[m
[32m+[m[32m        ':' +[m
[32m+[m[32m        colno +[m
[32m+[m[32m        '</div>',[m
[32m+[m[32m      '<pre style="' + ERROR_CODE_STYLE + '">' + error.stack + '</pre>',[m
[32m+[m[32m      '</div>',[m
[32m+[m[32m    ].join('')[m
[32m+[m
[32m+[m[32m    errorElement.style.position = 'fixed'[m
[32m+[m[32m    errorElement.style.zIndex = 1000000[m
[32m+[m[32m    errorElement.style.top = 0[m
[32m+[m[32m    errorElement.style.left = 0[m
[32m+[m[32m    errorElement.style.right = 0[m
[32m+[m[32m    errorElement.style.bottom = 0[m
[32m+[m[32m    errorElement.style.padding = '20px'[m
[32m+[m[32m    errorElement.style.background = 'rgba(16,17,18,0.66)'[m
[32m+[m[32m    errorElement.style.display = 'flex'[m
[32m+[m[32m    errorElement.style.alignItems = 'center'[m
[32m+[m[32m    errorElement.style.justifyContent = 'center'[m
[32m+[m
[32m+[m[32m    document.body.appendChild(errorElement)[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // NOTE:[m
[32m+[m[32m  // Yes â€“ we're attaching 2 error listeners below ðŸ‘€[m
[32m+[m[32m  // This is because React makes the same error throw twice (in development mode).[m
[32m+[m[32m  // See: https://github.com/facebook/react/issues/10384[m
[32m+[m
[32m+[m[32m  // Error listener #1[m
[32m+[m[32m  window.onerror = function (event, source, lineno, colno, error) {[m
[32m+[m[32m    _nextTick(function () {[m
[32m+[m[32m      if (_caughtErrors.indexOf(error) !== -1) return[m
[32m+[m
[32m+[m[32m      _caughtErrors.push(error)[m
[32m+[m
[32m+[m[32m      _handleError(error, {[m
[32m+[m[32m        event,[m
[32m+[m[32m        lineno,[m
[32m+[m[32m        colno,[m
[32m+[m[32m        source,[m
[32m+[m[32m      })[m
[32m+[m
[32m+[m[32m      _nextTick(function () {[m
[32m+[m[32m        var idx = _caughtErrors.indexOf(error)[m
[32m+[m
[32m+[m[32m        if (idx > -1) _caughtErrors.splice(idx, 1)[m
[32m+[m[32m      })[m
[32m+[m[32m    })[m
[32m+[m
[32m+[m[32m    // IMPORTANT: this callback must return `true` to prevent the error from being rendered in[m
[32m+[m[32m    // the browserâ€™s console.[m
[32m+[m[32m    return true[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // Error listener #2[m
[32m+[m[32m  window.addEventListener('error', function (event) {[m
[32m+[m[32m    if (_caughtErrors.indexOf(event.error) !== -1) return true[m
[32m+[m
[32m+[m[32m    _caughtErrors.push(event.error)[m
[32m+[m
[32m+[m[32m    _handleError(event.error, {[m
[32m+[m[32m      event,[m
[32m+[m[32m      lineno: event.lineno,[m
[32m+[m[32m      colno: event.colno,[m
[32m+[m[32m    })[m
[32m+[m
[32m+[m[32m    _nextTick(function () {[m
[32m+[m[32m      _nextTick(function () {[m
[32m+[m[32m        var idx = _caughtErrors.indexOf(event.error)[m
[32m+[m
[32m+[m[32m        if (idx > -1) _caughtErrors.splice(idx, 1)[m
[32m+[m[32m      })[m
[32m+[m[32m    })[m
[32m+[m
[32m+[m[32m    return true[m
[32m+[m[32m  })[m
[32m+[m[32m})()[m
