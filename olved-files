[36m@@ -0,0 +1,20 @@[m
[32m+[m[32mexport const apiVersion =[m
[32m+[m[32m  process.env.NEXT_PUBLIC_SANITY_API_VERSION || '2025-01-03'[m
[32m+[m
[32m+[m[32mexport const dataset = assertValue([m
[32m+[m[32m  process.env.NEXT_PUBLIC_SANITY_DATASET,[m
[32m+[m[32m  'Missing environment variable: NEXT_PUBLIC_SANITY_DATASET'[m
[32m+[m[32m)[m
[32m+[m
[32m+[m[32mexport const projectId = assertValue([m
[32m+[m[32m  process.env.NEXT_PUBLIC_SANITY_PROJECT_ID,[m
[32m+[m[32m  'Missing environment variable: NEXT_PUBLIC_SANITY_PROJECT_ID'[m
[32m+[m[32m)[m
[32m+[m
[32m+[m[32mfunction assertValue<T>(v: T | undefined, errorMessage: string): T {[m
[32m+[m[32m  if (v === undefined) {[m
[32m+[m[32m    throw new Error(errorMessage)[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return v[m
[32m+[m[32m}[m
