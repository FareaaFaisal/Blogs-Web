[32m+[m
[32m+[m[32mexport default function ThemeToggle() {[m
[32m+[m[32m  const { theme, setTheme } = useTheme();[m
[32m+[m[32m  return ([m
[32m+[m[32m    <button[m
[32m+[m[32m      onClick={() => setTheme(theme === "dark" ? "light" : "dark")}[m
