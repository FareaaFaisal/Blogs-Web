[32m+[m[32mimport { defineType, defineField, defineArrayMember } from 'sanity';[m
[32m+[m
[32m+[m[32mexport const post = defineType({[m
[32m+[m[32m  name: 'post',[m
[32m+[m[32m  type: 'document',[m
[32m+[m[32m  title: 'Post',[m
[32m+[m[32m  fields: [[m
[32m+[m[32m    defineField({[m
[32m+[m[32m      name: 'title',[m
[32m+[m[32m      type: 'string',[m
[32m+[m[32m      title: 'Post Title',[m
[32m+[m[32m      validation: (Rule) => Rule.required(),[m
[32m+[m[32m    }),[m
[32m+[m
[32m+[m[32m    // Slug field[m
[32m+[m[32m    defineField({[m
[32m+[m[32m      name: 'slug',[m
[32m+[m[32m      type: 'slug',[m
[32m+[m[32m      title: 'Slug',[m
[32m+[m[32m      options: {[m
[32m+[m[32m        source: 'title',[m
[32m+[m[32m        maxLength: 96,[m
[32m+[m[32m      },[m
[32m+[m[32m      validation: (Rule) => Rule.required(),[m
[32m+[m[32m    }),[m
[32m+[m
[32m+[m[32m    defineField({[m
[32m+[m[32m      name: 'summary',[m
[32m+[m[32m      type: 'text',[m
[32m+[m[32m      title: 'Summary',[m
[32m+[m[32m      validation: (Rule) => Rule.required(),[m
[32m+[m[32m    }),[m
[32m+[m
[32m+[m[32m    defineField({[m
[32m+[m[32m      name: 'image',[m
[32m+[m[32m      type: 'image',[m
[32m+[m[32m      title: 'Image',[m
[32m+[m[32m    }),[m
[32m+[m
[32m+[m[32m    defineField({[m
[32m+[m[32m      name: 'content',[m
[32m+[m[32m      type: 'array',[m
[32m+[m[32m      title: 'Content',[m
[32m+[m[32m      of: [[m
[32m+[m[32m        defineArrayMember({[m
[32m+[m[32m          type: 'block',[m
[32m+[m[32m        }),[m
[32m+[m[32m      ],[m
[32m+[m[32m    }),[m
[32m+[m[32m    defineField({[m
[32m+[m[32m        name: 'author',[m
[32m+[m[32m        type: 'reference',[m
[32m+[m[32m        title: 'Author',[m
[32m+[m[32m        to:[[m
[32m+[m[32m            {[m
[32m+[m[32m                type: 'author'[m
[32m+[m[32m            }[m
[32m+[m[32m        ][m
[32m+[m[32m      }),[m[41m  [m
[32m+[m
[32m+[m
[32m+[m[32m  ],[m
[32m+[m[32m});[m
